<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="csc.PII.triageapp"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="8"
        android:targetSdkVersion="17" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name="csc.PIII.triageapp.MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="csc.PIII.triageapp.MenuActivity"
            android:label="@string/title_activity_menu" >
        </activity>
        <activity
            android:name="csc.PIII.triageapp.UpdatePatient"
            android:label="@string/title_activity_update_patient" >
        </activity>
        <activity
            android:name="csc.PIII.triageapp.PriorityListActivity"
            android:label="@string/title_activity_priority_list" >
        </activity>
        <activity
            android:name="csc.PIII.triageapp.ArrivalListActivity"
            android:label="@string/title_activity_arrival_list" >
        </activity>
        <activity
            android:name="csc.PIII.triageapp.LoginActivity"
            android:label="@string/title_activity_login" >
        </activity>
        <activity
            android:name="csc.PIII.triageapp.NewUserActivity"
            android:label="@string/title_activity_new_user" >
        </activity>
        <activity
            android:name="csc.PIII.triageapp.AddPatientActivity"
            android:label="@string/title_activity_add_patient" >
        </activity>
        <activity
            android:name="csc.PIII.triageapp.UpdateVitalsActivity"
            android:label="@string/title_activity_update_vitals" >
        </activity>
        <activity
            android:name="csc.PIII.triageapp.LookupActivity"
            android:label="@string/title_activity_lookup" >
        </activity>
        <activity
            android:name="csc.PIII.triageapp.PhysMenuActivity"
            android:label="@string/title_activity_phys_menu" >
        </activity>
        <activity
            android:name="csc.PIII.triageapp.AddPrescriptionActivity"
            android:label="@string/title_activity_add_prescription" >
        </activity>
    </application>

</manifest>
